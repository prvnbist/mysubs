{
	"extends": "@mysubs/typescript-config/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/libs/*": ["libs/*"],
			"@/assets/*": ["assets/*"],
			"@/constants/*": ["constants/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		]
	},
	"include": [
		"next-env.d.ts",
		"next.config.js",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"postcss.config.ts"
	],
	"exclude": ["node_modules"]
}
