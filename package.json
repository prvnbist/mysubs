{
	"name": "tracksubs",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev",
		"lint": "biome lint ./apps ./packages",
		"format": "biome format --write .",
		"precommit": "yarn format && yarn lint"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@tracksubs/typescript-config": "*",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "yarn@1.22.5",
	"workspaces": ["apps/*", "packages/*"]
}
